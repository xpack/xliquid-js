{
  "name": "xliquid",
  "version": "0.1.0",
  "description": "Expand a liquid template",
  "main": "./index.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "bin": {
    "xsvd": "./bin/xsvd.js"
  },
  "scripts": {
    "prepublishOnly": "standard && npm run test-tap -s",
    "test": "standard && npm run test-tap -s",
    "test-coverage": "tap --coverage --reporter=classic --timeout 600 \"test/tap/*.js\"",
    "test-tap": "tap --reporter=classic --timeout 300 \"test/tap/*.js\"",
    "tap": "tap --reporter=spec --timeout 300",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpack/xliquid-js.git"
  },
  "keywords": [
    "liquid",
    "template"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "http://liviusdotnet.wordpress.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpack/xliquid-js/issues"
  },
  "homepage": "https://github.com/xpack/xliquid-js",
  "dependencies": {
    "@ilg/cli-start-options": "~0.1.17"
  },
  "devDependencies": {
    "standard": "~10.0.1",
    "tap": "~10.3.1"
  },
  "peerDependencies": {},
  "bundledDependencies": [],
  "optionalDependencies": {},
  "engines": {
    "node": ">7.7.0"
  }
}
